<h1>Making new story!</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@story) do |f| %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :author %>
      <%= f.text_field :author %>

      <%= f.label :summary %>
      <%= f.text_field :summary %>

      <%= f.label :url %>
      <%= f.text_field :url %>

      <%= f.submit "Edit this story!", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
